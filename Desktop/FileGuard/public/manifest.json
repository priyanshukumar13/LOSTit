{
  "manifest_version": 3,
  "name": "FileGuard AI",
  "version": "1.0.0",
  "description": "AI-powered file threat analysis and forensic scanning tool.",
  "action": {
    "default_popup": "index.html",
    "default_title": "Analyze File with FileGuard"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*"
  ]
}